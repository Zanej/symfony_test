<?php

namespace contentsBundle\Entity;

/**
 * conf_sezioni_box_config
 */
class conf_sezioni_box_config
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var int
     */
    private $idSezione;

    /**
     * @var int
     */
    private $rifBox;

    /**
     * @var string
     */
    private $titolo;

    /**
     * @var string
     */
    private $sottotitolo;

    /**
     * @var int
     */
    private $numBox;

    /**
     * @var string
     */
    private $tipologia;

    /**
     * @var string
     */
    private $titoloSottobox;

    /**
     * @var int
     */
    private $ordine;

    /**
     * @var array
     */
    private $parametri;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set idSezione
     *
     * @param integer $idSezione
     *
     * @return conf_sezioni_box_config
     */
    public function setIdSezione($idSezione)
    {
        $this->idSezione = $idSezione;

        return $this;
    }

    /**
     * Get idSezione
     *
     * @return int
     */
    public function getIdSezione()
    {
        return $this->idSezione;
    }

    /**
     * Set rifBox
     *
     * @param integer $rifBox
     *
     * @return conf_sezioni_box_config
     */
    public function setRifBox($rifBox)
    {
        $this->rifBox = $rifBox;

        return $this;
    }

    /**
     * Get rifBox
     *
     * @return int
     */
    public function getRifBox()
    {
        return $this->rifBox;
    }

    /**
     * Set titolo
     *
     * @param string $titolo
     *
     * @return conf_sezioni_box_config
     */
    public function setTitolo($titolo)
    {
        $this->titolo = $titolo;

        return $this;
    }

    /**
     * Get titolo
     *
     * @return string
     */
    public function getTitolo()
    {
        return $this->titolo;
    }

    /**
     * Set sottotitolo
     *
     * @param string $sottotitolo
     *
     * @return conf_sezioni_box_config
     */
    public function setSottotitolo($sottotitolo)
    {
        $this->sottotitolo = $sottotitolo;

        return $this;
    }

    /**
     * Get sottotitolo
     *
     * @return string
     */
    public function getSottotitolo()
    {
        return $this->sottotitolo;
    }

    /**
     * Set numBox
     *
     * @param integer $numBox
     *
     * @return conf_sezioni_box_config
     */
    public function setNumBox($numBox)
    {
        $this->numBox = $numBox;

        return $this;
    }

    /**
     * Get numBox
     *
     * @return int
     */
    public function getNumBox()
    {
        return $this->numBox;
    }

    /**
     * Set tipologia
     *
     * @param string $tipologia
     *
     * @return conf_sezioni_box_config
     */
    public function setTipologia($tipologia)
    {
        $this->tipologia = $tipologia;

        return $this;
    }

    /**
     * Get tipologia
     *
     * @return string
     */
    public function getTipologia()
    {
        return $this->tipologia;
    }

    /**
     * Set titoloSottobox
     *
     * @param string $titoloSottobox
     *
     * @return conf_sezioni_box_config
     */
    public function setTitoloSottobox($titoloSottobox)
    {
        $this->titoloSottobox = $titoloSottobox;

        return $this;
    }

    /**
     * Get titoloSottobox
     *
     * @return string
     */
    public function getTitoloSottobox()
    {
        return $this->titoloSottobox;
    }

    /**
     * Set ordine
     *
     * @param integer $ordine
     *
     * @return conf_sezioni_box_config
     */
    public function setOrdine($ordine)
    {
        $this->ordine = $ordine;

        return $this;
    }

    /**
     * Get ordine
     *
     * @return int
     */
    public function getOrdine()
    {
        return $this->ordine;
    }

    /**
     * Set parametri
     *
     * @param array $parametri
     *
     * @return conf_sezioni_box_config
     */
    public function setParametri($parametri)
    {
        $this->parametri = $parametri;

        return $this;
    }

    /**
     * Get parametri
     *
     * @return array
     */
    public function getParametri()
    {
        return $this->parametri;
    }
}
